{
  "private": true,
  "workspaces": [
    "bingo-cards-api-objects",
    "bingo-cards-api",
    "bingo-cards-ui",
    "bingo-meeting-objects",
    "bingo-meeting-objects-test",
    "bingo-meeting-console"
  ],
  "scripts": {
    "api": "cd bingo-cards-api && yarn start",
    "start": "cd bingo-cards-api && yarn start",
    "ui": "cd bingo-cards-ui && yarn start",
    "ui:build": "cd bingo-cards-ui && yarn build",
    "ui:lint": "cd bingo-cards-ui && yarn lint",
    "test": "cd bingo-meeting-objects-test && yarn test",
    "test:ui": "cd bingo-cards-ui && yarn test",
    "runConsole": "cd bingo-meeting-console && yarn start",
    "buildConsole": "yarn build && cd bingo-meeting-console && yarn build",
    "buildWebUI": "yarn build && cd bingo-cards-ui && yarn build:prod",
    "build:docker": "cd bingo-cards-ui && yarn build:docker",
    "build": "tsc -b ."
  },
  "dependencies": {
    "@angular/animations": "^8.2.3",
    "@angular/cdk": "^8.1.4",
    "@angular/material": "^8.1.4"
  }
}
